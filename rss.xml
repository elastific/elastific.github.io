&lt;/div&gt;&lt;div class="inner_cell"&gt;
&lt;div class="text_cell_render border-box-sizing rendered_html"&gt;
\begin{equation}
\begin{aligned}
\underbrace{
    \overbrace{
        \begin{bmatrix}
        Px              &amp;amp; 0               &amp;amp; \dots &amp;amp; 0     \\
        P_u \bar{\nu} K &amp;amp; 0               &amp;amp; \dots &amp;amp; 0     \\
        0               &amp;amp; P_x             &amp;amp; \dots &amp;amp; 0     \\
        0               &amp;amp; P_u \bar{\nu} K &amp;amp; \dots &amp;amp; 0     \\
        \dots           &amp;amp; \dots           &amp;amp; \dots &amp;amp; \dots
        \end{bmatrix}
    }^{\mathcal{M}}
    \begin{bmatrix}
    (A+B K)^0     \\
    (A+B K)^1     \\
    \vdots        \\
    (A+B K)^{n-1}
    \end{bmatrix}
}_{P_{x_N}}
\hat{x}_{k+N|k}
\leq
\underbrace{
    \begin{bmatrix}
    q_x    \\
    q_u    \\
    q_x    \\
    q_u    \\
    \vdots
    \end{bmatrix}
}_{q_{x_N}}
\end{aligned}
\end{equation}
&lt;/div&gt;
